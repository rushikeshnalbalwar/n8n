rderedActual = orderJsonKeys(n8nWorkflow);\n  \n  // Proper formatting\n  $input.all()[0].json.github_status = \"new\";\n  $input.all()[0].json.n8n_data_stringy = JSON.stringify(orderedActual, null, 2);\n}\n\n// Return items\nreturn $input.all();\n"},"id":"8ab20749-f850-4759-8a9c-b5b6f5ca2e69","name":"isDiffOrNew","type":"n8n-nodes-base.code","position":[640,1072],"typeVersion":1},{"parameters":{"dataType":"string","value1":"={{$json.github_status}}","rules":{"rules":[{"value2":"same"},{"value2":"different","output":1},{"value2":"new","output":2}]}},"id":"120801bf-4977-49d0-a9bb-b1592e1adfea","name":"Check Status","type":"n8n-nodes-base.switch","position":[1040,1072],"typeVersion":1},{"parameters":{},"id":"3eb0f7b3-7643-432a-bb77-e2f2a029c4e2","name":"Same file - Do nothing","type":"n8n-nodes-base.noOp","position":[1264,912],"typeVersion":1},{"parameters":{},"id":"ed2895fa-29fa-47a6-b290-d7192dea6011","name":"File is different","type":"n8n-nodes-base.noOp","position":[1264,1072],"typeVersion":1},{"parameters":{},"id":"fa32aabf-ed66-45c1-b830-b55d8db6b95e","name":"File is new","type":"n8n-nodes-base.noOp","position":[1264,1264],"typeVersion":1},{"parameters":{"resource":"file","owner":{"__rl":true,"mode":"","value":"={{ $('Config').first().item.repo_owner }}"},"repository":{"__rl":true,"mode":"","value":"={{ $('Config').first().item.repo_name }}"},"filePath":"={{ $('Config').first().item.repo_path }}{{ $json.subPath }}{{$('Execute Workflow Trigger').first().json.id}}.json","fileContent":"={{$('isDiffOrNew').item.json[\"n8n_data_stringy\"]}}","commitMessage":"={{$('Execute Workflow Trigger').first().json.name}} ({{$json.github_status}})"},"id":"43f558a4-52e9-41b1-9faa-40ea789e6712","name":"Create new file","type":"n8n-nodes-base.github","position":[1488,1264],"typeVersion":1,"webhookId":"088fbb2d-7ceb-4cc3-bdb7-f3f183725a63"},{"parameters":{"resource":"file","operation":"edit","owner":{"__rl":true,"mode":"","value":"={{ $('Config').first().item.repo_owner }}"},"reposi